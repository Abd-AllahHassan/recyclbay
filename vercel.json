{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://resyclbay-bckend.vercel.app/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
